{
  "name": "Bark Sender",
  "description": "Push text from webpages or clipboard to iOS devices using the Bark app. Includes shortcut key support.",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@lottiefiles/react-lottie-player": "^3.6.0",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "dayjs": "^1.11.13",
    "gsap": "^3.13.0",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.6.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@wxt-dev/module-react": "^1.1.3",
    "typescript": "^5.8.3",
    "wxt": "^0.20.6"
  }
}